<odoo>
  <record id="product_apart_form_view" model="ir.ui.view">
    <field name="name">product.apart</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_form_view" />
    <field name="arch" type="xml">
      <field name="company_id" position="after">
        <field name="idApart" string="Apartment"/>
        <field name="idVendor" string="Vendor"/>
      </field>
    </field>
  </record>
</odoo>